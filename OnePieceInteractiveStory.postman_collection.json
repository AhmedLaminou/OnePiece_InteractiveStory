			"item": [
				{
					"name": "Get User Profile",
					"request": {
						"method": "GET",
						"header": [
							{ "key": "apikey", "value": "{{supabase_key}}", "type": "text" },
							{ "key": "Authorization", "value": "Bearer {{supabase_key}}", "type": "text" }
						],
						"url": {
							"raw": "{{supabase_url}}/rest/v1/users?id=eq.YOUR_USER_ID&select=*",
							"host": ["{{supabase_url}}"],
							"path": ["rest", "v1", "users"],
							"query": [
								{ "key": "id", "value": "eq.YOUR_USER_ID" },
								{ "key": "select", "value": "*" }
